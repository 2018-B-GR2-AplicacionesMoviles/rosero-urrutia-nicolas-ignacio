<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Actualizar">

    <TextView
            android:text="Genero"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            app:layout_constraintEnd_toEndOf="@+id/eT_tallaDisfraz2"
            app:layout_constraintStart_toStartOf="@+id/eT_tallaDisfraz2" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/eT_tallaDisfraz2" app:layout_constraintHorizontal_bias="0.479"/>
    <Button
            android:text="Cancelar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_cancelar2"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/eT_precioDisfraz2" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="56dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.682"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/eT_generoDisfraz2"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintEnd_toEndOf="@+id/textView5" app:layout_constraintHorizontal_bias="0.538"
            android:layout_marginTop="28dp" app:layout_constraintTop_toBottomOf="@+id/textView5"/>
    <TextView
            android:text="Talla"
            android:layout_width="36dp"
            android:layout_height="24dp"
            android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="@+id/eT_nombreDisfraz2"
            app:layout_constraintEnd_toEndOf="@+id/eT_nombreDisfraz2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/eT_nombreDisfraz2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/eT_tallaDisfraz2"
            app:layout_constraintStart_toStartOf="@+id/textView6"
            app:layout_constraintEnd_toEndOf="@+id/textView6" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6" app:layout_constraintHorizontal_bias="0.508"/>
    <Button
            android:text="Eliminar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_Eliminar2"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/eT_precioDisfraz2"
            app:layout_constraintEnd_toStartOf="@+id/btn_cancelar2" android:layout_marginEnd="100dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.682"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/eT_nombreDisfraz2"
            app:layout_constraintEnd_toEndOf="@+id/textView8"
            app:layout_constraintStart_toStartOf="@+id/textView8" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8" app:layout_constraintHorizontal_bias="0.504"/>
    <TextView
            android:text="Precio"
            android:layout_width="wrap_content"
            android:layout_height="14dp"
            android:id="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/eT_generoDisfraz2"
            app:layout_constraintEnd_toEndOf="@+id/eT_generoDisfraz2"
            app:layout_constraintTop_toBottomOf="@+id/eT_generoDisfraz2" android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.52"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/eT_precioDisfraz2"
            app:layout_constraintStart_toStartOf="@+id/textView7"
            app:layout_constraintEnd_toEndOf="@+id/textView7" app:layout_constraintHorizontal_bias="0.52"
            android:layout_marginBottom="172dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="Nombre Disfraz"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <Button
            android:text="Actualizar"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:id="@+id/btn_Actualizar2" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/eT_precioDisfraz2" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="@+id/btn_Eliminar2"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="@+id/btn_cancelar2"
            app:layout_constraintHorizontal_bias="0.506"/>
</android.support.constraint.ConstraintLayout>